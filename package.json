{
	"name": "nios-scriptable",
	"version": "1.0.0",
	"description": "A node package that simulates iOS Scriptable's JS compiler for widgets.",
	"main": "src/index.js",
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./lib/index.d.ts",
				"default": "./lib/index.js"
			}
		},
		"./lib/nios-wrapper": {
			"import": {
				"types": "./lib/nios-wrapper/index.d.ts",
				"default": "./lib/nios-wrapper/index.js"
			}
		}
	},
	"files": [
		"lib/**/*"
	],
	"scripts": {
		"build": "rimraf lib && tsc"
	},
	"author": "SkinnyDevi",
	"license": "ISC",
	"devDependencies": {
		"rimraf": "^5.0.0",
		"typescript": "^5.0.4"
	},
	"dependencies": {
		"buffer": "^6.0.3"
	}
}